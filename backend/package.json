{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "test": "jest",
    "start": "nodemon --watch ./src --ext js --exec 'node' src/server.js",
    "migrate": "npx sequelize-cli db:migrate --config src/config/config.js --migrations-path src/migrations",
    "generate:migration": "prompt -p 'Migration name: ' -d name -- npx sequelize-cli migration:generate --name $name --migrations-path src/migrations",
    "generate:seed": "prompt -p 'Seed name: ' -d name -- npx sequelize-cli seed:generate --name $name --seeders-path src/seeders",
    "seed": "npx sequelize-cli db:seed:all --config src/config/config.js --seeders-path src/seeders"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.1.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.4.0",
    "mongoose": "^8.2.1",
    "npm": "^10.8.1",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prompt-cli": "^0.2.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  },
  "_moduleAliases": {
    "~config": "src/config",
    "~constants": "src/constants",
    "~controllers": "src/controllers",
    "~errors": "src/errors",
    "~middlewares": "src/middlewares",
    "~models": "src/models",
    "~repositories": "src/repositories",
    "~routes": "src/routes",
    "~services": "src/services",
    "~utils": "src/utils"
  }
}
